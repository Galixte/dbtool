{
    "name": "vse/dbtool",
    "type": "phpbb-extension",
    "description": "Allows you to check, optimize and repair phpBB's database tables from a phpMyAdmin-like interface in the Administration Control Panel.",
    "homepage": "https://github.com/VSEphpbb/dbtool",
    "version": "1.1.0-b2",
    "keywords": ["phpbb", "extension", "database", "optimize", "repair", "phpmyadmin"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Matt Friedman (VSE)",
            "homepage": "http://mattfriedman.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Database Optimize & Repair Tool",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0-RC2,<3.2.*@dev"
        },
        "version-check": {
            "host": "vsephpbb.github.io",
            "directory": "/dbtool",
            "filename": "version.json"
        }
    }
}
